<UserControl x:Class="Computer_Era_X.Views.Map"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:views="clr-namespace:Computer_Era_X.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <views:Form/>
        <Grid Margin="0,30,0,0">
            <WebBrowser x:Name="Browser" views:WebBrowserHelper.Body="{Binding BrowserSource}" Tag="{Binding BrowserTag}" Visibility="{Binding MapVisibility}" ScrollViewer.VerticalScrollBarVisibility="Disabled"/>
            <ContentControl Content="{Binding Map}"/>
        </Grid>
    </Grid>
</UserControl>
